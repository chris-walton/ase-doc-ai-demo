  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "kendo:auth": "npx kendo-ui-license activate",
    "docker:yarn": "",
    "docker:install": "yarn",
    "docker:globals": "yarn global add wrangler concurrently @angular/cli @angular-devkit/build-angular",
    "start:build": "ng build --watch --configuration development --output-path dist",
    "start:server": "wrangler pages dev dist/browser --compatibility-date=2021-09-20 --port 4200 --script-path _worker.js --log-level warn",
    "start": "npm run start:server",
    "start2": "concurrently \"npm run start:build\" \"npm run start:server\""
